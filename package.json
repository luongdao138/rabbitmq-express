{
  "name": "palbox",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "build:dev": "tsc -p tsconfig.json",
    "build:dev:clean": "rimraf ./dist && yarn build:dev",
    "serve:dev": "node dist/main.js",
    "start": "nodemon",
    "debug": "nodemon --inspect ./bin/bootstrap.js",
    "check-types": "tsc --pretty --noEmit -p tsconfig.dev.json && echo \"Types compiled\"",
    "format": "prettier --write \"src/**/*.{js,ts}\" \"**/*.md\"",
    "lint": "eslint --max-warnings 0 --report-unused-disable-directives --ext js,ts src",
    "lint:fix": "eslint --ext js,ts src --fix"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "dependencies": {
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.8.0",
    "rxjs": "^7.8.1",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/lodash": "^4.14.200",
    "@types/mongoose-delete": "^1.0.3",
    "@types/multer": "^1.4.10",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "swagger-autogen": "^2.23.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
