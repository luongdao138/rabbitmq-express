    "@types/multer": "^1.4.10",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "swagger-autogen": "^2.23.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
