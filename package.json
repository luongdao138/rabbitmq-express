{
  "name": "palbox",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "build:dev": "tsc -p tsconfig.json",
    "build:dev:clean": "rimraf ./dist && yarn build:dev",
    "serve:dev": "node dist/main.js",
    "start": "nodemon",
    "debug": "nodemon --inspect ./bin/bootstrap.js",
    "check-types": "tsc --pretty --noEmit -p tsconfig.dev.json && echo \"Types compiled\"",
    "format": "prettier --write \"src/**/*.{js,ts}\" \"**/*.md\"",
    "lint": "eslint --max-warnings 0 --report-unused-disable-directives --ext js,ts src",
    "lint:fix": "eslint --ext js,ts src --fix"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "dependencies": {
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.8.0",
    "axios": "^0.27.2",
    "basic-ftp": "^5.0.3",
    "bull": "^3.29.3",
    "cheerio": "^1.0.0-rc.9",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "elastic-apm-node": "^3.50.0",
    "elasticsearch": "^16.7.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-jwt": "^6.1.2",
    "express-list-endpoints": "^6.0.0",
    "facebook-nodejs-business-sdk": "^12.0.1",
    "fcm-node": "^1.6.1",
    "firebase-admin": "^9.11.0",
    "helmet": "^7.0.0",
    "ip": "^1.1.8",
    "jade": "^1.11.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.4",
    "mongoose": "^5.13.21",
    "mongoose-delete": "^1.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "rxjs": "^7.8.1",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.4.0",
    "socketio-jwt": "^4.6.2",
    "swagger-ui-express": "^5.0.0",
    "underscore": "^1.13.6",
